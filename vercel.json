{
    "version": 2,
    "builds": [
      { "src": "frontend/**/*", "use": "@vercel/static" },
      { "src": "api/*.mjs", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.mjs" },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }